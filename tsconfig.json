{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "declaration": true,
    "removeComments": false,
    "noUnusedLocals": true,
    "importHelpers": true,
    "lib": [
      "dom",
      "es5",
      "es2015.core"
    ]
  },
  "typedocOptions": {
    "name": "XDom",
    "mode": "file",
    "out": "doc",
    "theme": "node_modules/aiden-doc/typedoc_theme",
    "excludeNotExported": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "hideGenerator": true
  },
  "files": ["src/index.ts"]
}
