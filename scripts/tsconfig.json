{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitReturns": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": false,
    "removeComments": false,
    "importHelpers": true,
    "noEmitHelpers": true,
    "resolveJsonModule": true,
    "lib": [
      "dom",
      "es2015",
      "es2018"
    ]
  },
  "typedocOptions": {
    "mode": "modules",
    "out": "doc",
    "theme": "../../node_modules/aiden-doc/typedoc_theme",
    "excludeNotExported": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "hideGenerator": true
  }
}
