{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs"
  },
  "typedocOptions": {
    "name": "ImDOM",
    "mode": "modules",
    "out": "doc",
    "theme": "../../node_modules/aiden-doc/typedoc_theme",
    "excludeNotExported": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "hideGenerator": true
  }
}
